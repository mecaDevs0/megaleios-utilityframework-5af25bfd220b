{
        "version": "0.2.0",
        "configurations": [
            {
                "name": ".NET Core",
                "type": "coreclr",
                "request": "launch",
                "preLaunchTask": "build3",
                "program": "${workspaceFolder}/src/UtilityFramework.WebApi.Core3/bin/Debug/net5.0/UtilityFramework.WebApi.Core3.dll",
                "args": [],
                "cwd": "${workspaceFolder}/src/UtilityFramework.WebApi.Core3",
                "stopAtEntry": false,
                "internalConsoleOptions": "openOnSessionStart",
                "logging": {
                    "moduleLoad": false
                },
                "launchBrowser": {
                    "enabled": true,
                    "args": "${auto-detect-url}",
                    "windows": {
                        "command": "cmd.exe",
                        "args": "/C start ${auto-detect-url}"
                    },
                    "osx": {
                        "command": "open"
                    },
                    "linux": {
                        "command": "xdg-open"
                    }
                },
                "env": {
                    "ASPNETCORE_ENVIRONMENT": "Development"
                },
                "sourceFileMap": {
                    "/Views": "${workspaceFolder}/Views"
                }
            }
        ]
    }